@model Admin.Models.ViewModels.Events.EventsListVM

<h3>Фильтр</h3>
<table class="table">
    <thead>
        <tr>
            <th scope="col">id</th>
            <th scope="col">Заголовок</th>
            <th scope="col">Создан</th>
            <th scope="col">Кем создан</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var eve in Model.Events)
        {
            <tr>
                <th scope="row">@eve.EventId</th>
                <td>@eve.Title</td>
                <td>@eve.CreationDate.ToLongDateString()</td>
                <td>@eve.Creator.NormalizedUserName</td>
            </tr>
        }
    </tbody>

</table>
<div>
    страница @Model.PagingParam.CurrentPage из @Model.PagingParam.PageCount
</div>



